{
    "env": {
        "es6": false,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:node/recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 2022
    },
    "rules": {
        "no-console": "off",
        "node/no-missing-require": "off",
        "node/no-unsupported-features/es-syntax": "off",
        "no-unused-vars": "off",
        "no-process-exit": "off"
    },
    "plugins": ["prettier"]
}
